[#function convertType type language]
  [#if type == 'UUID' && language == 'csharp']
    [#return 'Guid'/]
  [/#if]
  [#return type/]
[/#function]

[#function methodParameters api language]
  [#local result = []]
  [#list api.params as param]
    [#if !param.constant??]
      [#local result = result + [convertType(param.javaType, language) + ' ' + param.name]/]
    [/#if]
  [/#list]
  [#return result?join(", ")/]
[/#function]